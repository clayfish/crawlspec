{
  "name":"dealsplus.com forum deals for geek",
  "model": "com.fp.ecommerce.model.Deal",
  "urls":[
    "index:https://www.dealsplus.com/All-Electronics_deals?page=1 !! category=Electronics | urlPath=div[class~=item itemTile]:has([class=likesCount]:contains(Likes)) div[class~=itemImg] a/@href"
	
  ],
  "listPath" : "div[class=itemHolder]",
  "fields":{
    "siteName": "##dealsplus.com",
    "title": "div[class=itemTitle]",
    "category": "$$category",
    "dealPrice": "div[class=itemSavings]",
    "finalUrl": "div[id=mainImage] a[href~=/go/rb]/@href",
    "numOfLike": "div[class=voteUpCount]/@data-vote-up-count",
    "desc": "div[class=howToContent]",
    "regularPrice": "div[class=listPrice] > span"
  }
}

